<views:ViewBase x:TypeArguments="viewModels:MainViewModel" x:Class="FitsArchiveUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitsArchiveUI"
        xmlns:viewModels="clr-namespace:FitsArchiveUI.ViewModels"
        xmlns:views="clr-namespace:FitsArchiveUI.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid d:DataContext="{d:DesignInstance Type=viewModels:MainViewModel}">
        <Button x:Name="RunButton" Content="{Binding Name}" HorizontalAlignment="Left" Margin="241,67,0,0" VerticalAlignment="Top" Width="75" Click="RunButton_Click"/>
        <TextBox x:Name="PathBox" HorizontalAlignment="Left" Height="23" Margin="52,64,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="174"/>

    </Grid>
</views:ViewBase>
